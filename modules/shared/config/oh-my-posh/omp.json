{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "palette": {
    "os": "p:text",
    "closer": "p:text",
    "pink": "#F5C2E7",
    "red": "#F38BA8",
    "mauve": "#CBA6F7",
    "peach": "#FAB387",
    "yellow": "#F9E2AF",
    "green": "#A6E3A1",
    "blue": "#89B4FA",
    "lavender": "#B4BEFE",
    "text": "#CDD6F4",
    "surface0": "#313244",
    "mantle": "#181825",
    "crust": "#11111b"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "",
          "foreground": "p:crust",
          "background": "p:blue",
          "template": "{{.Icon}} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:crust",
          "background": "p:blue",
          "template": "{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:crust",
          "background": "p:lavender",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:crust",
          "background": "p:mauve",
          "template": " {{ .HEAD }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:closer",
          "template": " \uf105"
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:text",
    "template": "\uf105 "
  }
}
